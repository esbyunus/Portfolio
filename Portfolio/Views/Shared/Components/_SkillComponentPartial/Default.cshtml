@model List<Portfolio.DL.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Yetenekler</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            <li>
                <div class="progress percent @item.Value"></div>
                <strong>@item.Title</strong>
            </li>
        }
    </ul>
</div>